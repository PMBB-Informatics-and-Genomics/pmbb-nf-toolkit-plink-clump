includeConfig 'plink_clump.config'

profiles {

    cluster {
        process.executor = 'lsf'
        process.queue = 'epistasis_normal'
        process.memory = '15GB'
        process.container = 'plink_clump.sif'
        singularity.enabled = true
        singularity.runOptions = '-B /project/,/static/'
    }
}